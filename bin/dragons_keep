#! /usr/bin/env ruby
# To change this template, choose Tools | Templates
# and open the template in the editor.
require 'rubygems'
begin
  require 'bundler'
  Bundler.setup
rescue 
end

# Set the path to load from the lib and main directory
path = File.expand_path(File.dirname(__FILE__))
$: << path
$: << File.join(path, "..", "lib")

load "dragons_keep.rb"

